{
  "source": {
    "includePattern": ".+\\.(ts|js)?$",
    "excludePattern": ".+\\.(test|spec).ts"
  },
  "plugins": ["node_modules/jsdoc-babel"],
  "babel": {
    "extensions": ["ts", "tsx", "js"],
    "ignore": ["**/*.(test|spec).ts"],
    "babelrc": false,
    "presets": [
      "@babel/preset-typescript"
    ]
  }
}